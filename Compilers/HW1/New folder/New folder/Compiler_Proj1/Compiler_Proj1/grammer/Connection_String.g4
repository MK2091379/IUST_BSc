grammar Connection_String;
start
   : connection_string EOF
   ;
connection_string
              : ((LS) + '=' (RS |LS)+ ';')*
              ;
LS
        : [a-zA-Z]+ (' ')*
        ;
RS
        :[a-zA-Z0-9]+ (S)*
        ;
S
    : '~' | '`' | '!' | '@' | '#' | '£' | '€' | '$' | '¢' | '¥' | '§' | '%' | '°' | '^' | '&' | '*' | '(' | ')' | '-' | '+' | '{' | '}' | '[' | ']' | '|' | '\\' | '/' | ':' | '<' | '>' | ',' | '.' | '?' | '_' | ' " ' | '_'
    ;

